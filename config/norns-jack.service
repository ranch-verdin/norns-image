[Unit]
After=sound.target

[Service]
Type=simple
User=we
Group=we
Environment="JACK_NO_AUDIO_RESERVATION=1"
LimitRTPRIO=95
LimitMEMLOCK=infinity
CPUAffinity=1,2,3
ExecStartPre=/home/we/norns/fettle_irqs.sh
ExecStart=/usr/bin/jackd -R -P 75 -d alsa -d hw:0 -r 48000 -n 2 -p 64 -S
ExecStartPost=/usr/bin/jack_wait -w -t 10
TimeoutStopSec=1

[Install]
WantedBy=norns.target
